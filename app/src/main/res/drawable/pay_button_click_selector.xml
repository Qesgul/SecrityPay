<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <gradient android:angle="180" android:endColor="@color/color_FF35EF9D" android:startColor="@color/color_FF63FAF2" />
            <corners android:radius="50dp"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <gradient android:angle="180" android:endColor="@color/color_FF28E390" android:startColor="@color/color_FF42EAD6" />
            <corners android:radius="50dp"/>
        </shape>
    </item>
</selector>